{
  "name": "Extension",
  "description": "An extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "48": "src/lang.png",
      "128": "src/lang.png"
    }
  },
  "icons": {
    "128": "src/lang.png"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "host_permissions": [
    "https://app.signnow.com/*",
    "http://localhost/*",
    "https://app3.signnow-dev.xyz/*"
  ]
}
